	<queryString>
		<![CDATA[SELECT
     e."nomefantasia" AS Nome,
     e."representante" AS Representante,
     e."telefone" AS Telefone,
     e."valor" AS Valor,
     a."nome" AS aluno_nome,
     a."cpf" AS aluno_cpf,
     a."instituicao" AS aluno_instituicao,
     a."situacao" AS aluno_situacao,
     a."telefone" AS aluno_telefone,
     a."email" AS aluno_email
FROM
     "public"."empresa" e INNER JOIN "public"."aluno" a ON e."nomefantasia" = a."transporte"
WHERE a."situacao" = 'Ativo'
ORDER BY
     e."nomefantasia" ASC]]>
	</queryString>
